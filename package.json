{
  "name": "globe-ar",
  "version": "0.1.1",
  "description": "UI component for Globe Data Visualization in AR",
  "license": "MIT",
  "unpkg": "dist/globe-ar.min.js",
  "main": "dist/globe-ar.common.js",
  "module": "dist/globe-ar.module.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vasturiano/globe-ar.git"
  },
  "homepage": "https://github.com/vasturiano/globe-ar",
  "keywords": [
    "augmented-reality",
    "globe",
    "ar.js",
    "aframe",
    "geo",
    "spherical",
    "projection",
    "orthographic"
  ],
  "author": {
    "name": "Vasco Asturiano",
    "url": "http://bl.ocks.org/vasturiano"
  },
  "bugs": {
    "url": "https://github.com/vasturiano/globe-ar/issues"
  },
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "dev": "rollup -c -w",
    "minify": "terser dist/globe-ar.js -o dist/globe-ar.min.js -c -m --comments '/Version/'",
    "build-module": "rollup -c rollup.config.module.js",
    "prepare": "npm run build && npm run minify && npm run build-module"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "accessor-fn": "^1.2.2",
    "aframe-globe-component": "^1.1.2",
    "kapsule": "^1.12.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.14",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "terser": "^4.4.3"
  }
}
