{
  "name": "globe-ar",
  "version": "1.0.0",
  "description": "UI component for Globe Data Visualization in AR",
  "license": "MIT",
  "unpkg": "dist/globe-ar.min.js",
  "main": "dist/globe-ar.common.js",
  "module": "dist/globe-ar.module.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vasturiano/globe-ar.git"
  },
  "homepage": "https://github.com/vasturiano/globe-ar",
  "keywords": [
    "augmented-reality",
    "globe",
    "ar.js",
    "aframe",
    "geo",
    "spherical",
    "projection",
    "orthographic"
  ],
  "author": {
    "name": "Vasco Asturiano",
    "url": "http://bl.ocks.org/vasturiano"
  },
  "bugs": {
    "url": "https://github.com/vasturiano/globe-ar/issues"
  },
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "dev": "rollup -c -w",
    "minify": "terser dist/globe-ar.js -o dist/globe-ar.min.js -c -m --comments '/Version/'",
    "build-module": "rollup -c rollup.config.module.js",
    "prepare": "npm run build && npm run minify && npm run build-module"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "aframe-globe-component": "^1.1.4",
    "kapsule": "^1.12.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.0.6",
    "rollup-plugin-babel": "^4.4.0",
    "terser": "^4.6.6"
  }
}
